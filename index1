<script>
    document.addEventListener("DOMContentLoaded", function() {
      var elements = document.querySelectorAll('.replace-content');
      
      elements.forEach(function(el) {
        var htmlContent = el.innerHTML;
        htmlContent = htmlContent.replace(/{platform}/g, 'Toloco');
        htmlContent = htmlContent.replace(/{domain}/g, 'Toloco');
        htmlContent = htmlContent.replace(/{phone}/g, '+1-8772769717');
        htmlContent = htmlContent.replace(/{description}/g, 'you can find everything you need.');
        el.innerHTML = htmlContent;
      });
    });
    </script>

